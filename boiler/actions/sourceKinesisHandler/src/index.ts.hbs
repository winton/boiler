import {
  KinesisStreamEvent,
  KinesisStreamRecord,
} from "aws-lambda"

export function {{lib}}(
  event: KinesisStreamEvent
): void {
  event.Records.forEach((record: KinesisStreamRecord) => {
    const payload = Buffer.from(
      record.kinesis.data,
      "base64"
    ).toString("ascii")

    // eslint-disable-next-line no-console
    console.log("Decoded payload:", payload)
  })
}
